{
  "name": "loopstudios-landing-page",
  "version": "1.0.0",
  "description": "FrontendMentor.io Project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass:watch": "sass --watch scss/styles.scss css/styles.css",
    "serve": "browser-sync start --server --files 'index.html, css/*.css'",
    "dev": "concurrently -k -n SASS,BS \"npm run sass:watch\" \"npm run serve\"",
    "build": "sass scss/styles.scss css/styles.css --style compressed",
    "deploy": "gh-pages -d ."

  },
